openssl x509 -outform der -in ~/gitlab.kurs.iad.crt -out ~/glki.crt
sudo apt install libnss3-tools
certutil -d sql:$HOME/.pki/nssdb -A -t "CT,C,C" -n ~/glki.crt -i ~/glki.crt
certutil -d sql:$HOME/.pki/nssdb -L
docker run --name fi33-nginx -v /home/lokaleradmin/Dokumente/git/klassenseite/:/usr/share/nginx/html:ro -p:81 -d nginx



apt-get -y install apt-transport-https ca-certificates curl \
    gnupg lsb-release
curl -fsSL https://download.docker.com/linux/ubuntu/gpg \
    | sudo gpg --dearmor -o /usr/share/keyrings/docker-archive-keyring.gpg
echo \
  "deb [arch=amd64 signed-by=/usr/share/keyrings/docker-archive-keyring.gpg] https://download.docker.com/linux/ubuntu \
  $(lsb_release -cs) stable" | sudo tee /etc/apt/sources.list.d/docker.list > /dev/null
apt-get update
apt-get -y install docker-ce docker-ce-cli containerd.io \
  git docker-compose
systemctl enable --now docker
adduser vagrant docker
